      "import": "./lib/index.js",
      "require": "./lib/index.js"
    }
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "jest": "^30.0.0",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "lint": "biome check .",
    "format": "biome format --write .",
    "prepare": "npm run build",
    "test": "bun test"
  },
  "files": [
    "lib",
    "src",
    "LICENSE",
    "README.md",
    "package.json",
    "tsconfig.json"
